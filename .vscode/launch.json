    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch-proxy",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/proxy/",
            "env": {
                "GOPATH": "${workspaceFolder}/vendor:${workspaceFolder}"
            },
            "args": ["-tls", "-strip-port", "-tracker", "http://127.0.0.1:8888/", "-listen", "0.0.0.0:8080", "-root", "/tmp"]
        },
        {
            "name": "Launch-tracker",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/tacker",
            "env": {
                "GOPATH": "${workspaceFolder}/vendor:${workspaceFolder}"
            },
            "args": ["-listen", "0.0.0.0:8888", "-tracker", "0.0.0.0:6881", "-root", "/tmp"]
        }
    ]
}
